{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint .",
    "serve": "source $HOME/.nvm/nvm.sh; nvm use 16; firebase emulators:start --only functions",
    "emulators:start": "source $HOME/.nvm/nvm.sh; nvm use 16; firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "build": "source $HOME/.nvm/nvm.sh; nvm use 16; npm i --package-lock-only --workspaces false; source ~/.zshrc; npm run lint && tsc"
  },
  "engines": {
    "node": "16"
  },
  "main": "dist/index.js",
  "dependencies": {
    "@types/firebase": "^3.2.1",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "axios": "^1.3.6",
    "firebase": "^10.5.0",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.21.1",
    "qs": "^6.10.3"
  },
  "devDependencies": {
    "eslint": "^8.13.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.27.5",
    "firebase-functions-test": "^0.2.0",
    "typescript": "^4.6.3"
  },
  "private": true
}
