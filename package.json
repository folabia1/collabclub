  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "build:functions": "cd functions && npm run build",
    "serve": "vite preview",
    "emulators:start": "npm run build:functions && firebase emulators:start --import=./saved-emulator-data --export-on-exit",
