{
  "name": "collabclub",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tanstack/react-query": "^5.0.0",
    "firebase": "^10.5.0",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.20.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "styled-components": "^6.1.0"
  },
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "build:functions": "cd ../functions && npm run build",
    "serve": "vite preview",
    "emulators:start": "source ~/.nvm/nvm.sh && nvm use 16 && npm run build:functions && firebase emulators:start --import=../saved-emulator-data --export-on-exit",
    "emulators:stop": "lsof -t -i:5001 -i:5002 -i:8080 -i:8085 -i:9000 -i:9099 -i:9199 -i:9090 | xargs kill -9"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.52.0",
    "eslint-plugin-react": "^7.33.2",
    "vite": "^4.5.0"
  }
}
